{
    "extends": "tslint:recommended",
    "rules": {
        "no-console": [
            false
        ],
        "variable-name": [
            true,
            "allow-leading-underscore",
            "ban-keywords",
            "check-format"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ]
    }
}